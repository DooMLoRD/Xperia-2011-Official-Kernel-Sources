msgstr "Nu s-a putut citi linkul simbolic „%s”: %s"

#: glib/gfileutils.c:1816
msgid "Symbolic links not supported"
msgstr "Linkurile simbolice nu sunt implementate"

#: glib/giochannel.c:1234
