	bool "Sensor mt9d112 (YUV 2M)"
	depends on MSM_CAMERA && !ARCH_MSM8X60
	default y
	---help---
	  MICRON 2M YUV Sensor

config IMX074
	bool "Sensor IMX074 (BAYER 13.5M)"
